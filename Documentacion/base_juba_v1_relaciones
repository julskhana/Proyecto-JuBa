-- Volcando estructura para tabla base_ca01.piscina
CREATE TABLE IF NOT EXISTS `piscina` (
  `id_piscina` int(10) NOT NULL AUTO_INCREMENT,
  `nombre` varchar(50) COLLATE utf8_spanish_ci NOT NULL DEFAULT '0',
  `descripcion` varchar(100) COLLATE utf8_spanish_ci DEFAULT '0',
  `ubicacion` varchar(50) COLLATE utf8_spanish_ci DEFAULT '0',
  `longitud_ancho` float NOT NULL DEFAULT '0',
  `longitud_largo` float NOT NULL DEFAULT '0',
  `area` float NOT NULL DEFAULT '0',
  `tipo` varchar(10) COLLATE utf8_spanish_ci DEFAULT '0',
  `forma` varchar(10) COLLATE utf8_spanish_ci DEFAULT '0',
  `poblacion_actual` int(100) DEFAULT '0',
  `id_empresa` int(10) NOT NULL DEFAULT '0',
  PRIMARY KEY (`id_piscina`),
  KEY `r_empresa_piscina` (`id_empresa`),
  CONSTRAINT `r_empresa_piscina` FOREIGN KEY (`id_empresa`) REFERENCES `empresa` (`id_empresa`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_spanish_ci;

