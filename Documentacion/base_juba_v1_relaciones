-- Volcando estructura para tabla base_ca01.producto
CREATE TABLE IF NOT EXISTS `producto` (
  `id_producto` int(11) NOT NULL AUTO_INCREMENT,
  `nombre` varchar(50) COLLATE utf8_spanish_ci NOT NULL,
  `nombre_cientifico` varchar(100) COLLATE utf8_spanish_ci DEFAULT NULL,
  `masa_total` float NOT NULL,
  `masa_actual` float NOT NULL,
  `masa_minima` float NOT NULL DEFAULT '0',
  `fecha_expiracion` timestamp NULL DEFAULT NULL,
  `id_ca` int(10) NOT NULL,
  PRIMARY KEY (`id_producto`),
