-- Volcando estructura para tabla base_ca01.comedor_auto
CREATE TABLE IF NOT EXISTS `comedor_auto` (
  `id_ca` int(11) NOT NULL AUTO_INCREMENT,
  `nombre` varchar(50) COLLATE utf8_spanish_ci NOT NULL DEFAULT '0',
  `descripcion` varchar(100) COLLATE utf8_spanish_ci NOT NULL DEFAULT '0',
  `capacidad_total` varchar(100) COLLATE utf8_spanish_ci NOT NULL DEFAULT '0',
  `nivel_bateria` float DEFAULT '100',
  `nivel_alimento` float DEFAULT '0',
  `nivel_o` float DEFAULT '0',
  `temperatura` float DEFAULT '0',
  `tipo_alimento` varchar(100) COLLATE utf8_spanish_ci DEFAULT '0',
  `distancia_recorrida` float DEFAULT '0',
  `numero_activaciones` int(100) DEFAULT '0',
  `estado` varchar(50) COLLATE utf8_spanish_ci DEFAULT '0',
  `id_piscina` int(10) NOT NULL DEFAULT '0',
  PRIMARY KEY (`id_ca`),
  KEY `r_piscina_ca` (`id_piscina`),
  CONSTRAINT `r_piscina_ca` FOREIGN KEY (`id_piscina`) REFERENCES `piscina` (`id_piscina`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_spanish_ci;

